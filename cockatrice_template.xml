<?xml version="1.0" encoding="UTF-8"?>
<cockatrice_deck version="1">
    <deckname>i{{.Name}}</deckname>
    <comments></comments>
    <zone name="main">
        {{ range $card := .Cards }}
        <card number="{{.Amount}}" name="{{.ID}}{{.Rarity}}"/>
        {{ end }}
    </zone>
</cockatrice_deck>
